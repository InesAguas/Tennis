<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:descendantFocusability="blocksDescendants" >

    <ImageView
        android:id="@+id/winner2Image"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/listSet3P2"
        app:layout_constraintStart_toStartOf="@+id/winner1Image"
        app:layout_constraintTop_toTopOf="@+id/listSet3P2"
        app:srcCompat="@drawable/mw_icon_checkmark_svg" />

    <TextView
        android:id="@+id/listNameTour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="TextView"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/listPlayer1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listNameTour" />

    <TextView
        android:id="@+id/listPlayer2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listPlayer1" />

    <TextView
        android:id="@+id/listSet1P1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="set1"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/listPlayer1"
        app:layout_constraintEnd_toStartOf="@+id/deleteButton"
        app:layout_constraintHorizontal_bias="0.543"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/listPlayer1"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/listSet2P1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_weight="1"
        android:text="set2"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/listSet1P1"
        app:layout_constraintStart_toEndOf="@+id/listSet1P1"
        app:layout_constraintTop_toTopOf="@+id/listSet1P1" />

    <TextView
        android:id="@+id/listSet3P1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_weight="1"
        android:text="set3"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/listSet2P1"
        app:layout_constraintStart_toEndOf="@+id/listSet2P1"
        app:layout_constraintTop_toTopOf="@+id/listSet2P1" />

    <TextView
        android:id="@+id/listSet1P2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="set1"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/listPlayer2"
        app:layout_constraintEnd_toStartOf="@+id/deleteButton"
        app:layout_constraintHorizontal_bias="0.543"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/listPlayer2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/listSet2P2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_weight="1"
        android:text="set2"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/listSet1P2"
        app:layout_constraintStart_toEndOf="@+id/listSet1P2"
        app:layout_constraintTop_toTopOf="@+id/listSet1P2" />

    <TextView
        android:id="@+id/listSet3P2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_weight="1"
        android:text="set3"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/listSet2P2"
        app:layout_constraintStart_toEndOf="@+id/listSet2P2"
        app:layout_constraintTop_toTopOf="@+id/listSet2P2" />

    <ImageButton
        android:id="@+id/deleteButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:contentDescription="Delete game"
        app:layout_constraintBottom_toBottomOf="@+id/listSet3P2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/listSet3P1"
        app:srcCompat="@android:drawable/ic_menu_delete" />

    <TextView
        android:id="@+id/listDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/deleteButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/winner1Image"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/listSet3P1"
        app:layout_constraintEnd_toStartOf="@+id/deleteButton"
        app:layout_constraintHorizontal_bias="0.24"
        app:layout_constraintStart_toEndOf="@+id/listSet3P1"
        app:layout_constraintTop_toTopOf="@+id/listSet3P1"
        app:srcCompat="@drawable/mw_icon_checkmark_svg" />

    <ImageView
        android:id="@+id/liveImage"
        android:layout_width="76dp"
        android:layout_height="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/listNameTour"
        app:layout_constraintStart_toEndOf="@+id/listNameTour"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/live_button" />

</androidx.constraintlayout.widget.ConstraintLayout>